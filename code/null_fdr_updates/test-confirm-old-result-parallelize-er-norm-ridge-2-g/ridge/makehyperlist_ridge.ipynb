    "import pickle\n",
    "import numpy as np\n",
    "\n",
    "hyperlist = list(10.0**(np.arange(-6, 7)))\n",
    "\n",
    "filename = \"hyperlist_ridge.p\"\n",
    "\n",
    "\n",
    "print \"Hyper list is \"\n",
    "print hyperlist\n",
    "\n",
    "pickle.dump(hyperlist, open(filename, 'wB'))\n",
    "\n",
    "print \"Dumped to \", filename\n",
    "print \"Loading: \"\n",
    "pickle.load(open(filename, 'rB'))"
   ]
